{
  "name": "js-cast",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "start": "electron .",
    "start:build": "npm run build:frontend && npm run build:backend && electron .",
    "build:all": "npm run build:frontend && npm run build:backend",
    "build:backend": "cd js-cast-backend && tsc",
    "build:frontend": "cd js-cast-frontend && npm run build",
    "backend:test": "cd js-cast-backend && npm run test",
    "frontend:storybook": "cd js-cast-frontend && npm run storybook",
    "frontend:test": "cd js-cast-frontend && npm run test",
    "format": "prettier --write .",
    "install": "npm i && cd js-cast-backend && npm i && cd js-cast-frontend && npm i",
    "package": "electron-builder --dir",
    "distribute": "electron-builder"
  },
  "build": {
    "appId": "user.inflamous.js-cast",
    "files": [
      "./dist/**/*"
    ]
  },
  "private": true,
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^12.0.9",
    "electron-builder": "^22.10.5",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {}
}
